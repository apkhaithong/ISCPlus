  object QInvPay: TFDQuery
    ConnectionName = 'HI_DBMS'
    SQL.Strings = (
      'SELECT A.INVNO,A.APCODE,A.LOCAT,A.INVDATE,A.INVDUE,A.NETTOTAL,'
      
        '     (A.SMPAY+A.SMCHQ) AS PAYAMT,A.TNOPAY,(A.NETTOTAL-A.SMPAY-A.' +
        'SMCHQ) AS BALANCE ,'
      '     (A.NETTOTAL-A.SMPAY-A.SMCHQ) AS DUEAMT,'
      '     B.APNAME FROM APINVOI A,APMAST B WHERE A.APCODE=B.APCODE')
    Left = 60
    Top = 4
  end
